{
    "name": "@nodecfdi/cfdiutils-common",
    "version": "1.0.1",
    "description": "Librería usada para contener las estructuras de datos comunes, y utilerías o funciones de ayuda común.",
    "main": "dist/cfdiutils-common.cjs.js",
    "module": "dist/cfdiutils-common.esm.js",
    "browser": "dist/cfdiutils-common.umd.js",
    "types": "dist/cfdiutils-common.d.ts",
    "files": [
        "dist"
    ],
    "homepage": "https://github.com/nodecfdi/cfdiutils-common",
    "repository": "https://github.com/nodecfdi/cfdiutils-common",
    "bugs": {
        "url": "https://github.com/nodecfdi/cfdiutils-common/issues"
    },
    "author": "OcelotlStudio <contacto@ocelotlstudio.com>",
    "license": "MIT",
    "keywords": [
        "common",
        "cfdi",
        "estructura",
        "struct",
        "xml"
    ],
    "scripts": {
        "dev": "rollup -c -w",
        "build": "rollup -c",
        "generate": "rimraf {build, dist} && yarn build && yarn build:types && yarn docs && rimraf build",
        "build:types": "tsc -p ./tsconfig.json --outDir build --declaration true && api-extractor run",
        "lint": "eslint --ext .js,.ts .",
        "test": "jest",
        "docs": "typedoc src --out docs"
    },
    "devDependencies": {
        "@microsoft/api-extractor": "^7.19.4",
        "@rollup/plugin-commonjs": "^21.0.2",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@rollup/plugin-typescript": "^8.3.1",
        "@types/jest": "^27.4.1",
        "@types/node": "^17.0.21",
        "@typescript-eslint/eslint-plugin": "^5.13.0",
        "@typescript-eslint/parser": "^5.13.0",
        "eslint": "^8.10.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.5.1",
        "prettier": "^2.5.1",
        "rollup": "^2.69.0",
        "ts-jest": "^27.1.3",
        "ts-node": "^10.6.0",
        "tslib": "^2.3.1",
        "typedoc": "^0.22.12",
        "typescript": "~4.5.5"
    },
    "dependencies": {
        "@xmldom/xmldom": "^0.8.1"
    }
}
